{x} = curve {data}
{data} = sin(pi(1 - {data}))^2

{head_rotX} @ {data} -> -0.8

{rightArm_rotX} @ {data} -> 0.8
{rightArm_rotY} @ {data} -> 0
{rightArm_rotZ} @ {data} -> 0.5

{leftArm_rotX} @ {data} -> 0.8
{leftArm_rotY} @ {data} -> 0
{leftArm_rotZ} @ {data} -> -0.5

{rightLeg_rotX} @ {data} -> 0.8
{rightLeg_rotZ} @ {data} -> 0.2

{leftLeg_rotX} @ {data} -> 0.8
{leftLeg_rotZ} @ {data} -> 0.2

out "pivot" (0, 0.5, 0)
out "rotate" (360{x}, 0, 1, 0)
