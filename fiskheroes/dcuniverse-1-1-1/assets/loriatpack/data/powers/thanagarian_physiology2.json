{
    "name": "Thanagarian Physiology",
   "modifiers": {
	"fiskheroes:regeneration": {
      "factor": 2
    },
	"fiskheroes:teleportation": {
      "canReachMoon": true,
      "range": 1.0,
      "soundEvents": {
        "TELEPORT": "fiskheroes:glow_teleport"
      }
    },
	"fiskheroes:blade": {
      "soundEvents": {
        "ENABLE": "fiskheroes:manta_blade_enable",
        "DISABLE": "fiskheroes:manta_blade_disable"
      }
    },
	"fiskheroes:shield": {
      "shield": {
        "health": 100.0,
        "regeneration": 10.0,
        "recoveryDelay": 30,
        "cooldown": 80
      },
      "coverage": {
        "yawRadius": 35.0,
        "pitchRadius": 35.0,
        "yawOffset": 0.0,
        "pitchOffset": 0.0
      },
      "knockback": 0.2,
      "soundEvents": {
        "DEFLECT": "fiskheroes:shield_deflect",
        "DISARM": "fiskheroes:shield_disarm"
      }
    },
	"fiskheroes:controlled_flight": {
      "speed": 0.1,
      "boostSpeed": 0.2,
      "canBoost": true,
	  "barrelRoll": {
        "duration": 14,
        "speed": 0.12,
        "drag": 0.03
      },
      "diveSpeedRetention": 0.5,
      "collision": {
        "blocks": {
          "stopFlying": false,
          "takeDamage": false
        },
        "entities": {
          "stopFlying": false,
          "dealDamage": true
        }
      },
      "knockback": 0.4,
      "soundEvents": {
        "ENABLE": ["fiskheroes:flight_loop", "loriatpack:flap_wings"],
        "BOOST": "fiskheroes:flight_boost"
      }
    }
  }
}