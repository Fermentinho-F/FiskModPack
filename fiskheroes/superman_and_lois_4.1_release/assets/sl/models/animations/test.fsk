{data} = sin(pi(1 - {data}))^2

{rightArm_posZ} += 0
{rightArm_posY} -= 0
{rightArm_posX} @ {data} -> -200

{leftArm_posZ} -= 0
{leftArm_posY} -= 0
{leftArm_posX} @ {data} -> -200

{rightLeg_posZ} += 0
{rightLeg_posY} -= 0
{rightLeg_posX} @ {data} -> -200

{leftLeg_posZ} -= 0
{leftLeg_posY} -= 0
{leftLeg_posX} @ {data} -> -200

{body_posZ} -= 0
{body_posY} -= 0
{body_posX} @ {data} -> -200

{head_posZ} -= 0
{head_posY} -= 0
{head_posX} @ {data} -> -200
