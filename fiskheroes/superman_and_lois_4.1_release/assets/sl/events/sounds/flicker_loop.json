{
  "parent": "fiskheroes:sound_base",
  "sound": "fiskheroes:modifier.superspeed.flicker.loop.comics",
  "condition": {
    "continue": "entity.getData('fiskheroes:speeding')"
  },
  "volume": "entity.getData('fiskheroes:speed') <= 3 ? 0.0 : 0.3 * (entity.getData('fiskheroes:speed') > 3 ? 1 : 0)",
  "pitch": "1.2 - Math.random() * 0.4",
  "loop": true,
  "fadeIn": 5,
  "fadeOut": 5
}
